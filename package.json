    "react-konva": "^17.0.2-0",
    "react-native": "^0.64.1",
    "react-scripts": "^4.0.3",
    "react-zdog": "^1.0.11",
    "sass": "^1.32.4",
    "three": "^0.128.0",
    "typescript": "^4.2.4",
    "zdog": "^1.1.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "chroma-js": "^2.1.1",
    "lal": "^5.3.7",
    "react-range": "^1.8.6",
    "react-resize-aware": "^3.1.0",
    "react-spring": "^9.1.2",
    "react-use": "^17.1.1",
